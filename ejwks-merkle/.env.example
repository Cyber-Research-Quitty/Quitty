APP_DB_PATH=./keys.db
REDIS_URL=redis://localhost:6379/0

# Root signer: "ed25519" (works everywhere) OR "dilithium2" (requires python-oqs)
ROOT_SIGNER=ed25519
ROOT_SIGNER_KEY_PATH=./root_signer_key.json

# Bloom filter settings (DoS shield)
BLOOM_BITS=1048576
BLOOM_HASHES=7


# Copy to .env --> cp .env.example .env
